<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <!-- <th>Release<br>Year</th> -->
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% tips.forEach(function(t) { %>
        <tr>
          <td><%= t.title %></td>
       
          <td><a href="/tips/<%= t._id %>">REPLY</a></td>
          <td>
            <form action="/tips/<%= t._id %>?_method=DELETE" method="POST">
              <button type="submit">DELETE</button>
            </form>
          </td>
          <td><a href="/tips/<%= t._id %>/edit">EDIT</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    



<%- include('../partials/footer') %>