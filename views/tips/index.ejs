<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <!-- <th>Release<br>Year</th> -->
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% tips.forEach(function(t) { %>
        <tr>
          <td><%= t.title %></td>
          <td><%= t.releaseYear %></td>
          <td><a href="/tips/<%= t._id %>">REPLY</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    



